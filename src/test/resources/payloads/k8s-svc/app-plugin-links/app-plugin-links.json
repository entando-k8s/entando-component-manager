{
  "_embedded": {
    "entandoAppPluginLinks": [
      {
        "kind": "EntandoAppPluginLink",
        "apiVersion": "entando.org/v1",
        "metadata": {
          "name": "my-app-to-plugin-link",
          "namespace": "my-namespace"
        },
        "spec": {
          "entandoAppNamespace": "my-namespace",
          "entandoAppName": "my-app",
          "entandoPluginNamespace": "plugin-namespace",
          "entandoPluginName": "plugin"
        },
        "entandoStatus": {},
        "_links": {
          "self": {
            "href": "http://localhost:9080/app-plugin-links/my-app-to-plugin-link"
          },
          "app": {
            "href": "http://localhost:9080/apps/my-app"
          },
          "plugin": {
